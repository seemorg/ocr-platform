{
  "name": "ocr.usul.ai",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "db:push": "turbo -F @usul-ocr/db push",
    "db:migrate": "turbo -F @usul-ocr/db migrate:dev",
    "db:studio": "turbo -F @usul-ocr/db studio",
    "dev": "turbo watch dev",
    "dev:next": "turbo watch dev -F @usul-ocr/web...",
    "format": "turbo run format --continue -- --cache --cache-location node_modules/.cache/.prettiercache",
    "format:fix": "turbo run format --continue -- --write --cache --cache-location node_modules/.cache/.prettiercache",
    "lint": "turbo run lint --continue -- --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:fix": "turbo run lint --continue -- --fix --cache --cache-location node_modules/.cache/.eslintcache",
    "lint:ws": "pnpm dlx sherif@latest",
    "postinstall": "pnpm lint:ws",
    "typecheck": "turbo run typecheck"
  },
  "devDependencies": {
    "@turbo/gen": "^2.0.8",
    "@usul-ocr/prettier-config": "workspace:*",
    "prettier": "catalog:",
    "turbo": "^2.0.8",
    "typescript": "catalog:"
  },
  "prettier": "@usul-ocr/prettier-config"
}
