{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json",
        "dist/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [
        "node_modules/.cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        "node_modules/.cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        "node_modules/.cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "migrate:dev": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "USUL_DATABASE_URL",
    "NEXTAUTH_SECRET",
    "NEXTAUTH_URL",
    "EMAIL_SERVER_USER",
    "EMAIL_SERVER_PASSWORD",
    "EMAIL_SERVER_HOST",
    "EMAIL_SERVER_PORT",
    "EMAIL_FROM",
    "NEXT_PUBLIC_OCR_SERVER_URL",
    "OCR_SERVER_API_KEY",
    "AIRTABLE_API_TOKEN",
    "AIRTABLE_APP_ID",
    "AZURE_OPENAI_DEPLOYMENT_NAME",
    "AZURE_OPENAI_KEY",
    "AZURE_OPENAI_RESOURCE_NAME",
    "NEXT_PUBLIC_PARTYKIT_HOST",
    "CLOUDFLARE_ACCOUNT_ID",
    "CLOUDFLARE_R2_ID",
    "CLOUDFLARE_R2_SECRET",
    "CLOUDFLARE_R2_BUCKET",
    "CLOUDFLARE_R2_BUCKET_URL",
    "NEXT_PUBLIC_AXIOM_DATASET",
    "NEXT_PUBLIC_AXIOM_TOKEN",
    "PORT"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}