{
  "name": "queue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "pnpm with-env node dist/index.js",
    "dev": "pnpm with-env tsup-node --watch --onSuccess \"node dist/index.js\"",
    "build": "pnpm with-env tsup-node",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@azure-rest/ai-document-intelligence": "1.0.0-beta.2",
    "@azure/core-auth": "^1.7.2",
    "@azure/openai": "^1.0.0-beta.12",
    "@bull-board/api": "^5.21.1",
    "@bull-board/hono": "^5.21.1",
    "@hono/node-server": "^1.12.0",
    "@t3-oss/env-core": "^0.10.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@usul-ocr/db": "workspace:*",
    "bullmq": "^5.10.3",
    "hono": "^4.5.1",
    "lru-cache": "^11.0.0",
    "pdf-lib": "^1.17.1",
    "pdf-to-img": "^4.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@usul-ocr/eslint-config": "workspace:*",
    "@usul-ocr/prettier-config": "workspace:*",
    "@usul-ocr/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.0.2",
    "tsx": "^3.12.7",
    "typescript": "catalog:"
  },
  "prettier": "@usul-ocr/prettier-config"
}