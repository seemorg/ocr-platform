{
  "name": "@usul-ocr/db",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./single": {
      "types": "./dist/single.d.ts",
      "import": "./dist/single.js"
    }
  },
  "scripts": {
    "postinstall": "pnpm generate",
    "build": "tsup",
    "dev": "tsup --watch",
    "generate": "prisma generate",
    "migrate:dev": "pnpm with-env prisma migrate dev",
    "migrate:deploy": "pnpm with-env prisma migrate deploy",
    "push": "pnpm with-env prisma db push --skip-generate",
    "studio": "pnpm with-env prisma studio",
    "with-env": "dotenv -e ../../.env --",
    "clean": "rm -rf .turbo dist node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint"
  },
  "dependencies": {
    "@prisma/client": "^5.21.0",
    "@t3-oss/env-core": "^0.10.1"
  },
  "devDependencies": {
    "@usul-ocr/eslint-config": "workspace:*",
    "@usul-ocr/prettier-config": "workspace:*",
    "@usul-ocr/tsconfig": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "prisma": "^5.21.0",
    "tsup": "^8.0.2",
    "typescript": "catalog:"
  },
  "prettier": "@usul-ocr/prettier-config"
}